@font-face {font-family: "iconfont";
    src: url('iconfont.eot?t=1531275814300'); /* IE9*/
    src: url('iconfont.eot?t=1531275814300#iefix') format('embedded-opentype'), /* IE6-IE8 */
    url('data:application/x-font-woff;charset=utf-8;base64,') format('woff'),
    url('iconfont.ttf?t=1531275814300') format('truetype'), /* chrome, firefox, opera, Safari, Android, iOS 4.2+*/
    url('iconfont.svg?t=1531275814300#iconfont') format('svg'); /* iOS 4.1- */
}

.iconfont {
    font-family:"iconfont" !important;
    font-size:16px;
    font-style:normal;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

.icon-back1:before { content: "\e679"; }

.icon-right:before { content: "\e6a8"; }

.sc-btn-icon {
    width: 76rpx !important;
    min-width: 76rpx !important;
    height: 76rpx !important;
    padding: 0;
    margin-right: 0;
    margin-left: 0;
    overflow: hidden;
    font-size: 38rpx;
    line-height: normal;
    border-radius: 50% !important;
    border-style: none;
}

.sc-btn {
    background-color: inherit;
    line-height: 66px;
    min-width: 88px;
    height: 36px;
    padding: 0 16px;
    margin: 0;
    font-size: 32rpx;
    border-radius: 2px;
}

.sc-icon {
    position: absolute;
    top: 50%;
    left: 50%;
    width: 54rpx !important;
    line-height: 54rpx;
    -webkit-transform: translate(-27rpx, -27rpx);
    transform: translate(-27rpx, -27rpx);
}

.sc-date-picker{
    box-sizing: border-box;
    display: flex;
    flex-direction: column;
    flex-wrap: nowrap;
    justify-content: center;
    align-items: start;
}

.sc-date-picker .sc-date-picker-header{
    width: 100%;
    box-sizing: border-box;
    display: flex;
    flex-direction: column;
    flex-wrap: nowrap;
    justify-content: start;
    align-items: start;
    padding: 32rpx 56rpx;
    background: #009688;
    color: #ffffff;
}

.sc-date-picker .sc-date-picker-container{
    width: 100%;
    overflow: hidden;
}

.sc-date-picker-header .sc-date-picker-year-select{
    font-size: 32rpx;
}

.sc-date-picker-header .sc-date-picker-datetime-select{
    font-size: 46rpx;
}

.sc-date-picker-container .sc-date-picker-arrow-action{
    box-sizing: border-box;
    display: flex;
    flex-direction: row;
    flex-wrap: nowrap;
    justify-content: space-between;
    align-items: center;
    width: 100%;
    padding: 16rpx 32rpx;
}

.sc-date-picker-container .sc-date-picker-datetime-show{
    color: #000000;
    font-size: 32rpx;
    line-height: 38px;
}

.sc-date-picker-container .sc-date-picker-date{
    padding: 16rpx 32rpx;
}

.sc-date-picker .sc-date-picker-action{
    margin: 16rpx 8rpx;
    display: flex;
    flex-direction: row;
    justify-content: flex-end;
    align-items: center;
}

.sc-date-picker-date .sc-date-picker-date-header{
    display: flex;
    flex-direction: row;
    flex-wrap: nowrap;
    justify-content: space-between;
    align-items: center;
    font-size: 32rpx;
    color: rgba(0, 0, 0, .54);
    padding-bottom: 16rpx;
}

.sc-date-picker-date-header view{
    display: inline-block;
    flex: 1 0;
    min-width: 76rpx;
    text-align: center;
}

.sc-date-picker-date-select{
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
    justify-content: flex-start;
    align-items: center;
}

.sc-date-picker-date-select .sc-date-picker-date-select-btn{
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    flex: 0 0 14.28%;
    min-width: 76rpx;
    text-align: center;
}

.sc-date-picker-date-select .sc-date-picker-date-select-btn.selected view{
    background-color: #666666;
    color: #ffffff;
}

.sc-date-picker-date-select-btn view{
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    vertical-align: middle;
    width: 76rpx;
    height: 76rpx;
    border-radius: 50%;
    /*background-color: #666666;*/
    color: #000000;
}

.sc-date-picker-date-select-btn view text{
    vertical-align: middle;
    font-size: 32rpx;
}

.sc-date-picker-year-select{
    display: flex;
    flex-direction: row;
    justify-content: flex-start;
    align-items: center;
}

.sc-date-picker-year-select .sc-date-picker-year-btn{
    width: 100%;
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    padding: 24rpx;
    font-size: 32rpx;
}

.sc-date-picker-year-select .sc-date-picker-year-btn.selected{
    font-size: 54rpx;
    color: #666666;
}